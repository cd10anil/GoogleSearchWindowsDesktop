<UserControl x:Class="GoogleSearch.SearchResultCard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="64" d:DesignWidth="300" Width="Auto" Height="Auto" MouseEnter="UserControl_MouseEnter" MouseLeave="UserControl_MouseLeave">
    <Grid>
        <Label x:Name="TitleLabel" Content="Title" VerticalAlignment="Top" Height="54" FontSize="20" Padding="16,9,5,5"/>
        <Label x:Name="ResultLink" Content="http://www.google.com" Margin="0,38,0,0" Foreground="#FF4A95D3" Padding="16,0,8,10"/>

    </Grid>
</UserControl>
